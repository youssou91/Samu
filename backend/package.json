{
  "name": "presence-management-api",
  "version": "1.0.0",
  "description": "API de gestion de présence médicale",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "dev:debug": "nodemon --exec babel-node --presets=@babel/preset-env src/server.js",
    "build": "babel src --out-dir dist --copy-files",
    "seed:test": "cross-env NODE_ENV=test node scripts/seed-test-data.js",
    "import:mock": "cross-env NODE_ENV=development node scripts/import-mock-data.js",
    "serve": "npm run build && npm start",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "seed": "babel-node src/utils/seed.js",
    "docker:build": "docker build -t presence-management-api .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs presence-management-api",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "node",
    "express",
    "mongodb",
    "jwt",
    "api",
    "medical",
    "appointment",
    "healthcare"
  ],
  "author": "Votre Nom <votre.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-utilisateur/presence-management.git"
  },
  "bugs": {
    "url": "https://github.com/votre-utilisateur/presence-management/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "type": "commonjs",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.28.0",
    "@babel/node": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.28.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.1",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/config/**"
    ]
  }
}
